{
  "name": "class-schedule",
  "version": "2.0.0",
  "private": true,
  "type": "module",
  "homepage": "https://horario.lafoz.dev/",
  "scripts": {
    "dev": "vite --host --force",
    "build": "tsc && vite build",
    "deploy": "npm run build && gh-pages -d dist",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview --port 8085",
    "pretty:all": "prettier --check \"{config,src,stories}/**/*{.js,.jsx,.ts,.tsx}\" || prettier --write \"{config,src,stories}/**/*{.js,.jsx,.ts,.tsx}\"",
    "pretty:staged": "pretty-quick --staged --pattern \"{config,src,stories}/**/*{.js,.jsx,.ts,.tsx}\"",
    "pretty:quick": "pretty-quick --pattern \"{config,src,stories}/**/*{.js,.jsx,.ts,.tsx}\"",
    "types:check": "tsc --pretty --noEmit --declaration",
    "types:check:watch": "npm run types:check -- --watch",
    "types:declaration": "tspc --project tsconfig.prod.json"
  },
  "dependencies": {
    "@heroui/form": "^2.1.9",
    "@heroui/react": "2.6.14",
    "@heroui/shared-icons": "2.1.2",
    "@heroui/system": "2.4.7",
    "@heroui/theme": "2.4.6",
    "@libsql/client": "^0.14.0",
    "@popperjs/core": "^2.11.8",
    "@remix-run/router": "^1.23.0",
    "canvas-confetti": "^1.9.3",
    "clsx": "^2.1.0",
    "framer-motion": "^12.4.1",
    "postcss": "^8.4.38",
    "react": "^18.2.0",
    "react-custom-roulette": "^1.4.1",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "react-hook-form": "^7.54.2",
    "react-router": "^6.23.0",
    "react-router-dom": "^6.23.0",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.2.2",
    "vite": "^7.1.7",
    "esbuild": "0.25.10"
  },
  "devDependencies": {
    "@types/canvas-confetti": "^1.9.0",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@types/react-helmet": "^6.1.11",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "prettier": "^2.8.8",
    "pretty-quick": "^3.1.1"
  }
}